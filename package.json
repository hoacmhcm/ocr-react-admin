{
  "name": "fiza-agency-portal",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "sync:static": "./syncStatic.sh -m '[agency-portal] build static prod'",
    "build": "vite build",
    "build:stg": "vite build --mode staging",
    "build:dev": "vite build --mode staging",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/, !(ui/assets)/**/}*.{tsx, ts}\" && git add -A .",
    "release dry run": "standard-version --release-as patch --dry-run",
    "release as version": "standard-version --release-as  1.1.0 --dry-run",
    "release prerelease": "standard-version --prerelease --dry-run",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "4.24.7",
    "dayjs": "^1.11.6",
    "lodash.clonedeep": "^4.5.0",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "8.0.4",
    "react-router-dom": "^6.8.1",
    "react-smooth-dnd": "^0.11.1",
    "react-use": "17.2.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "vite-plugin-html-config": "^1.0.11"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "antd-form-builder": "^2.1.4",
    "consola": "^2.15.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "prettier": "^2.8.4",
    "prettier-eslint-cli": "^7.1.0",
    "sass": "^1.69.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-babel": "^1.1.3",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-import": "^0.4.0",
    "vite-plugin-style-import": "^2.0.0"
  }
}
